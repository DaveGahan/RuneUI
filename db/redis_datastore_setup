// inspect console command
if (isset($argv[1])) {
    switch ($argv[1]) {
        case 'reset':
            redisDatastore($redis,'reset');
            sysCmd('/srv/http/db/redis_acards_details');
            break;
        case 'check':
            redisDatastore($redis,'check');
            break;
    }
} else {
    echo "RuneAudio Datastore reset\nredis_datastore_setup {reset|check}\n";
}
